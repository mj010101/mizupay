    "@solana-program/system": "^0.7.0",
    "@solana/kit": "^2.1.0",
    "@solana/react": "^2.1.0",
    "@wallet-standard/core": "^1.1.0",
    "@wallet-standard/react": "^1.0.0",
    "react": "^19",
    "react-dom": "^19",
    "react-error-boundary": "^5.0.0",
    "react-icons": "^5.5.0",
    "react-router-dom": "^7.5.0",
    "swr": "^2.3.3"
